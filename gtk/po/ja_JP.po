"subme=7:partitions=p8x8,b8x8,i8x8,i4x4:8x8dct=1:\n"
"deblock=0,0:trellis=1:psy-rd=1,0:aq-strength=1.0:\n"
"no-fast-pskip=0:no-dct-decimate=0:cabac=1"
msgstr ""
